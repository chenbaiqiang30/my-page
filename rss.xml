<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
<channel>
<title>小超RSS</title>
<link>https://chenbaiqiang30.github.io/my-page/</link>
<description>自动订阅源</description>
<language>zh-cn</language>

<script src="site-data.js"></script>
<script>
(async()=>{
    const {posts}=await loadSiteData();
    document.write(posts.map(p=>`
        <item>
            <title>${p.title}</title>
            <link>https://chenbaiqiang30.github.io/my-page/${p.url}</link>
        </item>
    `).join(""));
})();
</script>

</channel>
</rss>
